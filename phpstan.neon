parameters:
    level: 5
    paths:
        - src
        - tests
    excludePaths:
        - vendor/*
    ignoreErrors:
        - '#Parameter \#[0-9]+ \$[a-zA-Z]+ of class [a-zA-Z\\]+ constructor expects [a-zA-Z\\]+, Mockery\\MockInterface given\.#'
        - '#Call to an undefined method Mockery\\ExpectationInterface\|Mockery\\HigherOrderMessage::(once|with)\(\)\.#'
    checkMissingIterableValueType: false 